%header
  %style{:type => "text/css", :media => "screen"}
    = css
  %script{:type => "text/javascript"}
    = js
  %title Done - Kanban

%body

  %h1 #{board.title}
  
  .content
    %h2 All done tasks
    = render_done

    %h2 All completed tasks
    = render_complete

  .navigation
    = render_navigation

  .footer
    %p
      %em Generated on #{Time.now} by kanban.
